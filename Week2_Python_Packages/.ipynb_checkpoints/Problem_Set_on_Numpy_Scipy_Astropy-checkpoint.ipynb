    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.integrate import solve_ivp\n",
    "from astropy.constants import G as gravitational_constant\n",
    "from astropy.constants import M_sun\n",
    "\n",
    "# Constants\n",
    "G = gravitational_constant.value  # Gravitational constant\n",
    "M_sun = M_sun.value  # Mass of the Sun in kg\n",
    "\n",
    "# Function to compute the derivatives\n",
    "def earth_motion(t, state):\n",
    "    x, dxdt, y, dydt = state\n",
    "    r = np.sqrt(x**2 + y**2)\n",
    "    d2xdt2 = -G * M_sun / r**3 * x\n",
    "    d2ydt2 = -G * M_sun / r**3 * y\n",
    "    return [dxdt, d2xdt2, dydt, d2ydt2]\n",
    "\n",
    "# Initial conditions\n",
    "x0 = 1.4710e11  # Initial x position of Earth (meters)\n",
    "y0 = 0          # Initial y position of Earth (meters)\n",
    "vx0 = 0         # Initial velocity along x (m/s)\n",
    "vy0 = 2.9783e4  # Initial velocity along y (m/s)\n",
    "\n",
    "# Time span\n",
    "t_span = (0, 365 * 24 * 3600)  # 1 year in seconds\n",
    "\n",
    "# Initial state\n",
    "initial_state = [x0, vx0, y0, vy0]\n",
    "\n",
    "# Solve the differential equations\n",
    "sol = solve_ivp(earth_motion, t_span, initial_state, method='RK45', dense_output=True)\n",
    "\n",
    "# Extracting results\n",
    "t = np.linspace(0, 365 * 24 * 3600, 1000)\n",
    "x, vx, y, vy = sol.sol(t)\n",
    "\n",
    "# Plotting the trajectory\n",
    "plt.figure(figsize=(8, 6))\n",
    "plt.plot(x, y, label='Earth\\'s Orbit')\n",
    "plt.scatter(0, 0, color='yellow', marker='o', label='Sun')\n",
    "plt.title('Earth\\'s Orbit Around the Sun')\n",
    "plt.xlabel('x (meters)')\n",
    "plt.ylabel('y (meters)')\n",
    "plt.legend()\n",
    "plt.axis('equal')\n",
    "plt.grid(True)\n",
    "plt.show()\n"
