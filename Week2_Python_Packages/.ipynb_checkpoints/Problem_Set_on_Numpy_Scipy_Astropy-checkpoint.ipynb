   "source": [
    "#Now, calculate h\n",
    "import math as m\n",
    "h = m.acos(-m.tan(del_hg.radian)*m.tan(phi.radian))\n",
    "print(\"h = \",h)\n",
    "T = (m.degrees(2*h)/360)*u.day #conversion between sidereal and solar day\n",
    "T_in_h = T.to(u.h)\n",
    "print(\"T = \",T_in_h)"
